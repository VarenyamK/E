<h1>New Course</h1>
<%= form_for :course, url: courses_path do |f| %>
  <p>
  <%= f.label :class_id %>:
  <%= f.text_field :class_id %><br />
  </p>

  <p>
  <%= f.label :section %>:
  <%= f.text_field :section %><br />
  </p>

  <p>
  <%= f.label :component %>:
  <%= f.text_field :component %><br />
  </p>

  <p>
  <%= f.label :days %>:
  <%= f.text_field :days %><br />
  </p>

  <p>
  <%= f.label :time %>:
  <%= f.text_field :time %><br />
  </p>

  <p>
  <%= f.label :location %>:
  <%= f.text_field :location %><br />
  </p>

  <p>
  <%= f.label :professor %>:
  <%= f.text_field :professor %><br />
  </p>

  <p>
  <%= f.submit %>
  </p>
<% end %>